        "widget.shadow": "#080A0F"
    },
    "tokenColors": [
        {
            "name": "Cadet Blue",
            "scope": [
                "meta.brace",
                "punctuation.definition.array",
                "punctuation.definition.binding-pattern",
                "punctuation.definition.block",
                "punctuation.definition.dictionary",
                "punctuation.definition.string",
                "punctuation.definition.tag",
                "punctuation.section",
                "punctuation.separator",
                "punctuation.support",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#abb2bf"
            }
        },
        {
            "name": "Iron",
            "scope": [
                "support.type.property-name",
                "support.variable",
                "variable",
                "entity.name.variable"
            ],
            "settings": {
                "foreground": "#D2D6db"
            }
        },
        {
            "name": "Orange-Red",
            "scope": [
                "keyword.var",
                "keyword.const",
                "keyword.type",
                "keyword.struct",
                "keyword.interface",
                "keyword.function",
                "keyword.other.definition",
                "markup.inline.raw",
                "punctuation.parenthesis.named.begin.regexp",
                "punctuation.parenthesis.named.end.regexp",
                "punctuation.parenthesis.non-capturing.begin.regexp",
                "punctuation.parenthesis.non-capturing.end.regexp",
                "storage.type",
                "support.constant",
                "support.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#e67e22"
            }
        },
        {
            "name": "Periwinkle",
            "scope": [
                "constant",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#6c71c4"
            }
        },
        {
            "name": "Shuttle Gray",
            "scope": [
                "comment"
            ],
            "settings": {
                "foreground": "#5c6370"
            }
        },
        {
            "name": "Olivine",
            "scope": [
                "string",
                "markup.quote",
                "markup.bold",
                "markup.italic"
            ],
            "settings": {
                "foreground": "#f1c40f"
            }
        },
        {
            "name": "Raven",
            "scope": [
                "string.quoted.docstring"
            ],
            "settings": {
                "foreground": "#737c8c"
            }
        },
        {
            "name": "Green",
            "scope": [
                "entity.name.class",
                "entity.name.function",
                "entity.name.type",
                "entity.other.attribute-name",
                "entity.other.inherited-class",
                "markup.heading.setext",
                "meta.function-call.generic",
                "support.function",
                "support.other.escape.special.regexp"
            ],
            "settings": {
                "foreground": "#2ecc71"
            }
        },
        {
            "name": "Blue",
            "scope": [
                "storage",
                "support.function.builtin",
                "variable.language"
            ],
            "settings": {
                "foreground": "#3498db"
            }
        },
        {
            "name": "Red",
            "scope": [
                "entity.name.tag",
                "entity.name.section",
                "keyword",
                "punctuation.definition.heading",
                "punctuation.definition.keyword"
            ],
            "settings": {
                "foreground": "#e74c3c"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold.markdown",
                "punctuation.definition.bold.markdown",
                "keyword.control"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Italic",
            "scope": [
                "comment",
                "markup.italic.markdown",
                "punctuation.definition.italic.markdown"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        }
    ]
}